const mongoose = require('mongoose');

const Schema = mongoose.Schema;

const markerSchema = new Schema({
    houseID: {
        type: String,
        required: true,
        trim: true
    },
    Text: {
        type: String,
        required: true,
        trim: true
    },
    locationName: {
        type: String,
        required: true,
        trim: true
    }
})

module.exports = Teleporter = mongoose.model('Marker', markerSchema);