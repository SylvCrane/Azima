const mongoose = require('mongoose');

const Schema = mongoose.Schema;

const showcaseImageSchema = new Schema({
    imageName: {
        type: String,
        required: true,
        trim: true
    },
    houseID: {
        type: String,
        required: true,
        trim: true
    }
});

module.exports = showcaseImage = mongoose.model('showcaseimage', showcaseImageSchema);